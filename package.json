{
	"name": "n8n-nodes-learningsuite",
	"version": "1.0.0",
	"description": "n8n node for LearningSuite API",
	"keywords": [
		"n8n-community-node-package",
		"learningsuite",
		"learningsuite.io",
		"learning",
		"education",
		"agentur-systeme.de",
		"courses"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials --ext .ts",
		"lint:fix": "eslint nodes credentials --ext .ts --fix",
		"lint:node": "npx nodelinter --target=nodes/LearningSuite/LearningSuite.node.ts",
		"lint:all": "npm run lint && npm run lint:node",
		"test": "npm run lint:all && npm run build",
		"prepublishOnly": "npm run build && npm run lint:all -s"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/LearningSuiteApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LearningSuite/LearningSuite.node.js",
			"dist/nodes/LearningSuite/LearningSuiteTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
