{
	"name": "@rjsebening/n8n-nodes-learningsuite",
	"version": "0.1.5",
	"description": "n8n node for LearningSuite API",
	"keywords": [
		"n8n-community-node-package",
		"learningsuite",
		"learningsuite.io",
		"learning",
		"education",
		"courses"
	],
	"license": "MIT",
	"homepage": "https://github.com/rjsebening/n8n-nodes-learningsuite#readme",
	"author": {
		"name": "JÃ¶rg Sebening",
		"email": "dev@joerg-sebening.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rjsebening/n8n-nodes-learningsuite.git"
	},
	"bugs": {
		"url": "https://github.com/rjsebening/n8n-nodes-learningsuite/issues"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials --ext .ts",
		"lint:fix": "eslint nodes credentials --ext .ts --fix",
		"lint:all": "npm run lint && npm run lint:node"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/LearningSuiteApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LearningSuite/LearningSuite.node.js",
			"dist/nodes/LearningSuite/LearningSuiteTrigger.node.js",
			"dist/nodes/LearningSuitePollingTrigger/LearningSuitePollingTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"publishConfig": {
		"access": "public"
	}
}
